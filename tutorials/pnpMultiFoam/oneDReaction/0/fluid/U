FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{

    fluidOuterWall
    {
        type            fixedValue;//pressureInletOutletVelocity;
        value           $internalField;
    }

    fluid_to_solid
    {
        type            fixedValue;
        value           $internalField;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


